cloud:
  aws:
    enabled: true
    credentials:
      access-key: ACCESS_KEY # this is for local testing, pls use AWS Secret Manager
      secret-key: SECRET_KEY # this is for local testing, pls use AWS Secret Manager
    region: AWS_REGION
    s3:
      enabled: true
      template:
        bucket:
          name: s3-bucket-eric6166
        signature-duration-minute: 10 # minute
    sqs:
      enabled: true
      template:
        queue:
          standard:
            name: sqs-queue-standard-eric6166
          fifo:
            name: sqs-queue-fifo-eric6166.fifo
            message-group-id: sqs-message-group-id-eric6166
        delay-seconds: 0
        max-number-of-messages: 10
